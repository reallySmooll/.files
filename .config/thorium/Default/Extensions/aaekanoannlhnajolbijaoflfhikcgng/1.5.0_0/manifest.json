{
   "action": {
      "default_popup": "charcoal_settings_popup.html"
   },
   "content_scripts": [ {
      "css": [ "styles/stylesheet.css" ],
      "js": [ "js/settings.js", "js/init.js", "js/charcoal_settings.js" ],
      "matches": [ "*://*.messenger.com/*" ]
   } ],
   "description": "Unofficial Messenger dark mode. Easily swap between dark and light mode.",
   "icons": {
      "128": "assets/icon@128.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Charcoal: Dark Mode for Messenger",
   "permissions": [ "storage" ],
   "short_name": "Charcoal",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.5.0",
   "web_accessible_resources": [ {
      "matches": [ "*://*/*" ],
      "resources": [ "assets/charcoal-messenger.svg", "assets/facebook-messenger.svg", "assets/midnight-messenger.svg", "assets/deepblue-messenger.svg", "charcoal_settings.html", "onboarding_dropdown.html" ]
   } ]
}
