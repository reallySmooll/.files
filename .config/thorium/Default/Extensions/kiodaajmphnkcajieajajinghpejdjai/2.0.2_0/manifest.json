{
   "action": {
      "default_icon": {
         "16": "/assets/images/icon16.png",
         "24": "/assets/images/icon24.png",
         "32": "/assets/images/icon32.png"
      },
      "default_popup": "/html/popup.html",
      "default_title": "Popup Blocker Pro"
   },
   "background": {
      "service_worker": "/js/sw.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "/assets/css/toast.css" ],
      "js": [ "/js/content.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "description": "__MSG_extDescription__",
   "homepage_url": "http://popup-blocker.org",
   "host_permissions": [ "*://*/*" ],
   "icons": {
      "128": "/assets/images/icon128.png",
      "16": "/assets/images/icon16.png",
      "48": "/assets/images/icon48.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 3,
   "name": "__MSG_extName__",
   "options_page": "html/options.html",
   "permissions": [ "storage", "webRequest", "declarativeNetRequest", "tabs", "contextMenus" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.0.2",
   "web_accessible_resources": [ {
      "matches": [ "*://*/*" ],
      "resources": [ "/assets/images/*.png" ]
   }, {
      "matches": [ "*://*/*" ],
      "resources": [ "js/inject.js", "js/cancel-inject.js" ]
   } ]
}
